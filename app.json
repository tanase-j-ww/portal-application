{
  "expo": {
    "name": "portal_application",
    "slug": "portal_application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.github.tanase_j_ww.portal_application",
      "versionCode": 1000000,
      "config": {
        "intentFilters": [
          {
            "action": "VIEW",
            "data": [
              {
                "scheme": "geo"
              }
            ],
            "category": ["DEFAULT"]
          }
        ]
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "331d09b9-588f-4767-8bea-569e5a5fd070"
      }
    }
  }
}
